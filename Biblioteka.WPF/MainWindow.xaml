<local:MetroReactiveWindow x:TypeArguments="viewModels:MainViewModel" x:Class="Biblioteka.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Biblioteka.WPF"
        xmlns:reactiveUi="http://reactiveui.net"
        xmlns:viewModels="clr-namespace:Biblioteka.WPF.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800" MinWidth="800" MinHeight="500">
    <reactiveUi:RoutedViewHost 
        x:Name="RoutedViewHost"
        HorizontalContentAlignment="Stretch"
        VerticalContentAlignment="Stretch" HorizontalAlignment="Center" VerticalAlignment="Center" >
    </reactiveUi:RoutedViewHost>
</local:MetroReactiveWindow>
